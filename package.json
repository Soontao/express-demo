{
  "name": "express-demo",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "4.5.0"
  },
  "scripts": {
    "start": "node ./www.js --use_strict",
    "compile": "babel src --out-dir bin --source-maps inline -D --no-comments",
    "postinstall": "npm run installFront&&npm run gendoc",
    "installFront": "node ./node_modules/bower/bin/bower install",
    "clean": "",
    "gendoc": "node ./node_modules/apidoc/bin/apidoc -i src -o doc"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "et-al": "^1.0.6",
    "express": "~4.13.4",
    "hbs": "~4.0.0",
    "morgan": "~1.7.0",
    "nodemailer": "^2.6.4",
    "sequelize": "^3.24.3",
    "serve-favicon": "~2.3.0",
    "sqlite3": "^3.1.4"
  },
  "devDependencies": {
    "apidoc": "^0.16.1",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2017": "^6.14.0",
    "babel-register": "^6.14.0",
    "bower": "^1.7.9"
  }
}